            justify-content: center;
        }

        .slider {
            width: 800px;
            height: 500px;
            border-radius: 5px;
            overflow: hidden;
        }

        .slide {
            width: 600%;
            height: 500px;
            display: flex;
        }

            .slide input {
                display: none;
            }

        .st {
            width: 20%;
            transition: 2s;
        }

            .st img {
                width: 800px;
                height: 500px;
            }

        .nav-m {
            position: absolute;
            width: 800px;
            margin-top: -40px;
