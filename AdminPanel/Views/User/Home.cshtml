        .button a {
            color: #6665ee;
            font-weight: 500;
        }

        .m-btn {
            border: 2px solid #6d44b8;
            padding: 5px;
            border-radius: 10px;
            cursor: pointer;
            transition: 3s;
        }

            .m-btn:not(:last-child) {
